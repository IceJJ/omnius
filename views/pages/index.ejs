.hero {
  width: 100%;
  height: 100vh;
  overflow: hidden;
}
.hero video {
  object-fit: cover;
  width: 100%;
  height: 100%;
}
.hero .hero-content {
  position: absolute;
  top: 35%;
width: 99%;
height: 100%;
transform: translateX(0%);
z-index: 9;
text-align: center;
}
.hero .hero-content h1 {
font-family: "Montserrat", sans-serif !important;
font-weight: 600;
letter-spacing: -1px;
visibility: inherit;
transition: none 0s ease 0s;
text-align: center;
margin: 0px;
padding: 4px 0px;
font-size: 70px;
min-height: 0px;
min-width: 0px;
max-height: none;
max-width: none;
opacity: 1;
transform: translate3d(0px, 0px, 0px);
transform-origin: 50% 50% 0px;
color: rgb(255, 255, 255);

}
.hero .hero-content h3 {
    font-size: 25px;
    text-align: center;
    color: #fff;
    line-height: 30px;
    margin-top: 20px;
}

.grid-overlay {
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAACCAYAAAEFsT2yAAAACXBIWXMAAAsSAAALEgHS3X78AAAAFElEQVR4nGNhYGBwYIET////ZwAAETIDym2lzEcAAAAASUVORK5CYII=);
  background-size: 2px 2px;
  background-position: 0 0;
  background-repeat: repeat;
}

@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@-moz-keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.hero::after {
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAACCAYAAAEFsT2yAAAACXBIWXMAAAsSAAALEgHS3X78AAAAFElEQVR4nGNhYGBwYIET////ZwAAETIDym2lzEcAAAAASUVORK5CYII=);;
    content: '';
    top: 0;
    height: 100%;
    position: absolute;
    display: inline-block;
    bottom: 0;
    left: 0;
    right: 0;
}

.card .carousel-item {
